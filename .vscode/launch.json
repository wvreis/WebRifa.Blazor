    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/WebRifa.Blazor/WebRifa.Blazor/bin/Debug/net8.0/WebRifa.Blazor.dll",
            "args": [],
            "cwd": "${workspaceFolder}//WebRifa.Blazor/WebRifa.Blazor",
            "stopAtEntry": false,
            "console": "internalConsole",
            "serverReadyAction": {
                "action": "debugWithEdge",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)" ,
                //"uriFormat": "%s/swagger"                          
            }
        }
    ]
}